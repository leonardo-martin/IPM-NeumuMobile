{
  "name": "teleneumu",
  "version": "0.0.1",
  "author": "Atos AMS Team",
  "description": "Conectar pessoas, negócios e tecnologia para ajudar a melhorar o mundo em que vivemos é o nosso propósito e com esse objetivo desenvolvemos o app TeleNeuMu para o Instituto Pedro Molina (IPM), referência na pesquisa científica de doenças neuromusculares.",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android --variant=developmentDebug",
    "android:staging": "cd android && ./gradlew clean && cd .. && react-native run-android --variant=developmentRelease",
    "android:prod": "cd android && ./gradlew clean && cd .. && react-native run-android --variant=productionRelease",
    "build-android-staging": "cd android && ./gradlew clean assembleDevelopmentRelease && cd ..",
    "build-android-prod": "cd android && ./gradlew clean assembleProductionRelease && cd ..",
    "ios": "react-native run-ios --scheme TeleNeuMuDevelopment --configuration Debug",
    "ios:staging": "react-native run-ios --scheme TeleNeuMuStaging --configuration Staging",
    "ios:prod": "react-native run-ios --scheme TeleNeuMuProduction --configuration Production",
    "codepush:deploykey": "appcenter codepush deployment list -a ${OWNER_APP_NAME:=''} -k",
    "codepush:publish": "appcenter codepush release-react -a ${OWNER_APP_NAME:=''} -d ${ENVIRONMENT:='Staging'}",
    "codepush:clear": "appcenter codepush deployment clear -a ${OWNER_APP_NAME:=''} ${ENVIRONMENT:=''}",
    "clean:project": "react-native clean --include android,cocoapods,watchman,npm,metro",
    "clean:android": "react-native clean --include android",
    "clean:ios": "react-native clean --include cocoapods,watchman",
    "pods:install": "cd ios && pod install && cd ..",
    "pods:update": "cd ios && pod update && cd ..",
    "pods:clean": "cd ios && pod deintegrate && rm -rf Podfile.lock && cd ..",
    "test": "jest",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --reset-cache true",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios --reset-cache true",
    "prettier:write": "npx prettier --write **/*.{ts,tsx,json,js,jsx} && npx prettier --write *.{ts,tsx,json,js,jsx}",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@eva-design/eva": "^2.1.1",
    "@hookform/error-message": "^2.0.0",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-clipboard/clipboard": "^1.10.0",
    "@react-native-community/datetimepicker": "^5.1.0",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/drawer": "^6.4.2",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@ui-kitten/components": "^5.1.2",
    "@ui-kitten/date-fns": "^5.1.2",
    "@ui-kitten/eva-icons": "^5.1.2",
    "axios": "^0.27.2",
    "date-fns": "1.30.1",
    "gerador-validador-cpf": "^5.0.2",
    "jwt-decode": "^3.1.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.0.0",
    "react-hook-form": "7.32.2",
    "react-native": "0.69.1",
    "react-native-animatable": "^1.3.3",
    "react-native-calendar-events": "^2.2.0",
    "react-native-calendars": "^1.1285.0",
    "react-native-code-push": "^7.0.4",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-config": "^1.4.6",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^10.0.2",
    "react-native-document-picker": "^8.1.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keychain": "^8.1.1",
    "react-native-modalize": "^2.0.13",
    "react-native-pager-view": "^5.4.9",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "^2.9.0",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.14.0",
    "react-native-svg": "^12.3.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.23.0",
    "react-redux": "7.2.8",
    "redux": "^4.1.2",
    "socket.io-client": "^4.5.1",
    "swr": "^1.2.2",
    "to": "^0.2.9",
    "update": "^0.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/runtime": "^7.12.15",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.4.0",
    "@types/react-native": "^0.67.4",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "babel-jest": "27.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "jest": "27.0.2",
    "metro-react-native-babel-preset": "^0.71.2",
    "prettier": "^2.6.2",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
